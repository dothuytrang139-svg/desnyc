local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- GUI si√™u nh·ªè g·ªçn
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "MiniMenu"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 180, 0, 70)
Frame.Position = UDim2.new(0, 20, 0, 200)
Frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
Frame.Active = true
Frame.Draggable = true
Instance.new("UICorner", Frame).CornerRadius = UDim.new(0,10)

-- N√∫t 1: Unwalk
local UnwalkButton = Instance.new("TextButton", Frame)
UnwalkButton.Size = UDim2.new(0.5,-10,1,-10)
UnwalkButton.Position = UDim2.new(0,5,0,5)
UnwalkButton.Text = "Unwalk"
UnwalkButton.TextColor3 = Color3.fromRGB(255,255,255)
UnwalkButton.BackgroundColor3 = Color3.fromRGB(80,20,20)
UnwalkButton.Font = Enum.Font.SourceSansBold
UnwalkButton.TextSize = 16
Instance.new("UICorner", UnwalkButton).CornerRadius = UDim.new(0,8)

-- N√∫t 2: FFlag Toggle
local FFlagButton = Instance.new("TextButton", Frame)
FFlagButton.Size = UDim2.new(0.5,-10,1,-10)
FFlagButton.Position = UDim2.new(0.5,5,0,5)
FFlagButton.Text = "FFlag OFF"
FFlagButton.TextColor3 = Color3.fromRGB(255,255,255)
FFlagButton.BackgroundColor3 = Color3.fromRGB(20,20,80)
FFlagButton.Font = Enum.Font.SourceSansBold
FFlagButton.TextSize = 16
Instance.new("UICorner", FFlagButton).CornerRadius = UDim.new(0,8)

