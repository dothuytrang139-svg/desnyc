local enabled = false

FFlagButton.MouseButton1Click:Connect(function()
    enabled = not enabled
    if enabled then
        FFlagButton.Text = "FFlag ON"
        FFlagButton.BackgroundColor3 = Color3.fromRGB(20,80,20)

        for _, f in ipairs(flags) do
            local name, value = f[1], f[2]
            pcall(function()
                setfflag(name, tostring(value))
            end)
        end

    else
        FFlagButton.Text = "FFlag OFF"
        FFlagButton.BackgroundColor3 = Color3.fromRGB(20,20,80)
    end
end)
